///100 days of RTL///

///Abilash P///
 
///1 bit comparator///

module bit_1_comp(a_in, b_in, a_less_b, a_great_b, a_equal_b);

input  a_in, b_in;

output a_less_b, a_great_b, a_equal_b;

assign a_less_b  =  ~(a_in) & b_in;

assign a_great_b =  (a_in) & ~(b_in);

assign a_equal_b =   ~(a_in ^ b_in);

endmodule
 
